{
  "name": "uniprot-website",
  "version": "0.0.0",
  "main": "./src/ui/index.tsx",
  "repository": "",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --config webpack.dev.js",
    "build": "webpack --mode production --config webpack.prod.js",
    "check-types": "tsc",
    "tslint": "tslint -c tslint.json 'src/**/*.ts' --project tsconfig.json",
    "test": "yarn check-types && yarn tslint && jest",
    "clear-test-cache": "jest --clearCache"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "IE 11"
  ],
  "jest": {
    "transform": {
      "^.+\\.(tsx|js|ts)?$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "jest-css-modules-transform"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "@geneontology/ribbon": "^1.9.32",
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.15.0",
    "core-js": "^3.1.4",
    "d3": "^5.9.2",
    "franklin-sites": "0.0.3-dev.44",
    "idx": "^2.5.2",
    "litemol": "^2.4.2",
    "npm-run-all": "^4.1.2",
    "path": "^0.12.7",
    "prop-types": "^15.6.2",
    "protvista-datatable": "^2.1.13",
    "protvista-manager": "^2.1.13",
    "protvista-navigation": "^2.1.13",
    "protvista-sequence": "^2.1.13",
    "protvista-structure": "^2.1.13",
    "protvista-track": "^2.1.14",
    "query-string": "^6.2.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "url-join": "^4.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@testing-library/react": "^8.0.4",
    "@types/d3": "^5.7.2",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.13",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.12",
    "@types/react-router-dom": "^4.3.1",
    "@types/url-join": "^4.0.0",
    "@types/uuid": "^3.4.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.3",
    "jest": "^23.6.0",
    "jest-css-modules-transform": "^2.1.1",
    "jest-enzyme": "^6.1.2",
    "node-sass": "^4.9.3",
    "react-test-renderer": "^16.5.2",
    "redux-mock-store": "^1.5.3",
