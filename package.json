    "axios": "0.22.0",
    "classnames": "2.3.1",
    "core-js": "3.18.1",
    "d3": "5.16.0",
    "deep-freeze": "0.0.1",
    "franklin-sites": "0.0.165",
    "history": "4.10.1",
    "idb": "6.1.4",
    "interaction-viewer": "3.4.0",
