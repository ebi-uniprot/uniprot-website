    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typesafe-actions": "^3.0.0",
    "typescript": "^3.2.2",
    "webpack": "^4.20.2",
