{
  "name": "uniprot-website",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://ebi-uniprot.github.io/uniprot-website/",
  "license": "MIT",
  "scripts": {
    "start": "npm run start:prod",
    "start:dev": "webpack serve --env API_PREFIX=https://wwwdev.ebi.ac.uk/uniprot/beta/api --mode development",
    "start:prod": "webpack serve --env API_PREFIX=https://rest.uniprot.org --mode development",
    "start:fallback": "webpack serve --env API_PREFIX=http://hx-rke-wp-webadmin-02-worker-1.caas.ebi.ac.uk:31393 --mode development",
    "build:dev": "webpack --env API_PREFIX=https://rest.uniprot.org --env PUBLIC_PATH=/uniprot/front-end/dev/ --env GIT_BRANCH=$CI_COMMIT_BRANCH --mode development",
    "build:prod": "webpack --env API_PREFIX=https://rest.uniprot.org --env GIT_BRANCH=$CI_COMMIT_BRANCH --mode production",
    "lint-fix": "eslint src --ext .ts,.tsx --fix",
    "analyse": "webpack-bundle-analyzer build/stats.json",
    "jest-coverage-ratchet": "jest-coverage-ratchet",
    "coverage-bump": "npm-run-all test:unit jest-coverage-ratchet",
    "prettier": "prettier src --write",
    "test": "npm-run-all --continue-on-error test:lint test:types test:unit",
    "test:lint": "eslint src --ext .ts,.tsx --cache --cache-location 'node_modules/.cache'",
    "test:types": "tsc",
    "test:unit": "jest --coverage",
    "test:functional": "jest --config src/tests/jest.config.json src/tests/functional",
    "test:visual": "jest --config src/tests/jest.config.json src/tests/visual",
    "clear-test-cache": "jest --clearCache",
    "prepare": "husky install"
  },
  "browserslist": "defaults, cover 80% in CN, not dead",
  "jest": {
    "coverageReporters": [
      "json-summary",
      "lcov",
      "text-summary",
      "clover"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 82.14,
        "statements": 81.92,
        "functions": 82.08,
        "branches": 69.53
      }
    },
    "transform": {
      "^.+\\.(jsx|tsx|js|ts)?$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "jest-css-modules-transform"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!franklin-sites|p-map|aggregate-error|clean-stack).+\\.js"
    ],
    "testPathIgnorePatterns": [
      "/__mocks__/",
      "<rootDir>/\\..+\\/"
    ],
    "coveragePathIgnorePatterns": [
      "/__test-helpers__/",
      "<rootDir>/\\..+\\/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy",
      "^/(.*)$": "<rootDir>/src/$1",
      "^d3$": "<rootDir>/node_modules/d3/",
      "^react$": "<rootDir>/node_modules/react/",
      "^react-router-dom$": "<rootDir>/node_modules/react-router-dom/",
      "^timing-functions$": "<rootDir>/node_modules/timing-functions/",
      "^rheostat$": "<rootDir>/node_modules/rheostat/lib/Slider.js",
      "^rheostat/initialize$": "<rootDir>/node_modules/rheostat/lib/initialize.js",
      "^franklin-sites$": "<rootDir>/node_modules/franklin-sites/src/components/",
      "^lodash-es$": "lodash"
    },
    "globals": {
      "BASE_URL": "/",
      "API_PREFIX": "/<testing>/api",
      "GIT_COMMIT_HASH": "#git_commit_id"
    },
    "snapshotSerializers": [
      "<rootDir>/jest.serializer.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "globalSetup": "<rootDir>/jest.global.setup.ts",
    "testEnvironment": "jsdom"
  },
  "lint-staged": {
    "*.+(ts|tsx|js|jsx|css|scss)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@sentry/react": "7.12.1",
    "@sentry/tracing": "7.12.1",
    "@sentry/types": "7.12.1",
    "@swissprot/rhea-reaction-visualizer": "0.0.21",
    "@swissprot/swissbiopics-visualizer": "0.0.28",
    "@types/gtag.js": "0.0.11",
    "@types/marked": "4.0.7",
    "axios": "0.27.2",
    "classnames": "2.3.1",
    "core-js": "3.25.1",
    "d3": "5.16.0",
    "deep-freeze": "0.0.1",
    "franklin-sites": "0.0.205",
    "front-matter": "4.0.2",
    "history": "4.10.1",
    "idb": "7.0.2",
    "interaction-viewer": "3.5.9",
    "lite-youtube-embed": "0.2.0",
    "lodash-es": "4.17.21",
    "marked": "4.1.0",
