    "protvista-utils": "3.0.0",
    "protvista-variation": "3.0.0",
    "protvista-variation-adapter": "3.1.2",
    "query-string": "6.13.6",
    "react": "17.0.1",
    "react-beautiful-dnd": "13.0.0",
    "react-dom": "17.0.1",
    "react-redux": "7.2.2",
    "react-router-dom": "5.2.0",
    "react-spring": "8.0.27",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "svg-url-loader": "6.0.0",
    "timing-functions": "2.0.1",
