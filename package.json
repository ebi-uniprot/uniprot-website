    "protvista-variation-adapter": "3.5.6",
    "protvista-zoom-tool": "3.2.2",
    "query-string": "7.1.1",
    "react": "17.0.2",
    "react-beautiful-dnd": "13.1.1",
    "react-diff-viewer": "3.1.1",
    "react-dom": "17.0.2",
    "react-helmet-async": "1.3.0",
    "react-router-dom": "5.3.0",
    "sanitize-html": "2.7.1",
